<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.saas.titan.modules.work.dao.WorkMasterDao">

    <select id="getList"
            parameterType="java.lang.String"
            resultType="com.saas.titan.modules.work.dto.WorkMasterDto">
        select
            user_id,
            time,
            work
        from
            work_master
    </select>

</mapper>
