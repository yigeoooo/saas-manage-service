<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.saas.titan.modules.approve.dao.VacationMasterDao">

    <select id="getPage" resultType="com.saas.titan.modules.approve.dto.VacationMasterPageDto">
        select
            begin_time,
            end_time,
            days,
            reason,
            status,
            types
        from
            vacation_master
        ${ew.customSqlSegment}
    </select>

    <select id="getAllPage" resultType="com.saas.titan.modules.approve.dto.VacationMasterAllPageDto">
        select
            begin_time,
            end_time,
            days,
            reason,
            types,
            user_id
        from
            vacation_master
        ${ew.customSqlSegment}
    </select>

</mapper>
