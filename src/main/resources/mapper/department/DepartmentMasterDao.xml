<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.saas.titan.modules.department.dao.DepartmentMasterDao">

    <select id="getPage" resultType="com.saas.titan.modules.department.dto.DepartmentDto">
        select
            id,
            department_name,
            department_code,
            manager,
            insert_time
        from
            department_master
        ${ew.customSqlSegment}
    </select>

</mapper>
